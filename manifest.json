{
    "name": "YourBias",
    "version": "1.0",
    "description": "Find your bias.",
    "permissions": [
        "history",
        "storage"
    ],
    "background": {
        "scripts": ["scan.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "menu.html",
        "default_icon": {
            "128": "logo.png"
        }
    },
    "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
    "manifest_version": 2
}
